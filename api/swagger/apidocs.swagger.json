            "name": "tenant",
            "description": "tenant under which this user will be created",
            "in": "path",
            "required": true,
            "type": "string"
          },
          {
            "name": "username",
            "description": "user ID",
            "in": "path",
            "required": true,
            "type": "string"
          },
          {
            "name": "email",
            "description": "email address of the user",
            "in": "query",
            "required": false,
            "type": "string"
          },
          {
            "name": "firstname",
