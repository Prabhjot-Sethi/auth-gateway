            "in": "path",
            "required": true,
            "type": "string"
          },
          {
            "name": "offset",
            "description": "offset from where to start the list of users",
            "in": "query",
            "required": false,
            "type": "integer",
            "format": "int32"
          },
          {
            "name": "limit",
